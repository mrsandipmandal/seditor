{
    "name": "open-php/seditor",
    "version": "1.0.0",
    "description": "SEditor - A lightweight, dynamic, and dependency-free WYSIWYG editor for PHP applications.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/mrsandipmandal/seditor.git",
    "authors": [
        {
            "name": "Sandip Mandal",
            "email": "mr_sandip@zohomail.in"
        }
    ],
    "keywords": [
        "laravel",
        "blade",
        "components",
        "bootstrap",
        "ui",
        "editor",
        "wysiwyg",
        "seditor",
        "open-php",
        "compact"
    ],
    "autoload": {
        "psr-4": {
            "OpenPHP\\SEditor\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenPHP\\SEditor\\SEditorServiceProvider"
            ]
        }
    },
    "scripts": {
        "publish-assets": "OpenPHP\\SEditor\\Installer::copyAssets"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}